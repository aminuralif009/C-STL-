#include<bits/stdc++.h>
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
    deque<int>a;
    a.push_back(1);
    a.push_back(2);
    a.push_back(3);
    a.push_back(4);
    a.push_front(0);
    a.push_back(5);

    deque<int>::iterator x;
    x = a.begin()+3;
    a.erase(x);
    deque<int>::iterator y;
    y = a.end();
    a.insert(y,6);


    for(int i = 0; i<a.size(); i++)
    {
        cout<<a[i]<<" ";
    }
    cout<<endl;
    cout<<a.front()<<endl;
    cout<<a.back()<<endl;
    cout<<a.front()+a.back()<<endl;

    a.clear();

    if(a.empty())
    {
        cout<<"HERU"<<endl;
    }
    else
    {
        cout<<"Alright"<<endl;
    }
}
